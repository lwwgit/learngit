+
+asmlinkage int sys_test_five(int num)
+{
+	switch(num) {
+		case 0:
+			printk(KERN_EMERG"This is Syscall Test Five. 0 KERN_EMERG\n");
+			break;
+		case 1:
+			printk(KERN_EMERG"This is Syscall Test Five. 1 KERN_ALERT\n");
+			break;
+		case 2:
+			printk(KERN_EMERG"This is Syscall Test Five. 2 KERN_CRIT\n");
+			break;
+		case 3:
+			printk(KERN_EMERG"This is Syscall Test Five. 3 KERN_ERR\n");
+			break;
+		case 4:
+			printk(KERN_EMERG"This is Syscall Test Five. 4 KERN_WARNING\n");
+			break;
+		case 5:
+			printk(KERN_EMERG"This is Syscall Test Five. 5 KERN_NOTICE\n");
+			break;
+		case 6:
+			printk(KERN_EMERG"This is Syscall Test Five. 6 KERN_INFO\n");
+			break;
+		case 7:
+			printk(KERN_EMERG"This is Syscall Test Five. 7 KERN_DEBUG\n");
+			break;
+		default:
+			printk("err num %d. the num should be between 0~7\n", num);
+	}
+	return 1;
+}
